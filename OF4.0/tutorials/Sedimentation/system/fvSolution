FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    "gamma|alpha.*"
    {
        nAlphacCorr      0;
        nAlphadCorr      1;
        nAlphaSubCycles  1;
		cAlpha			 1;

        MULESCorr       no;
        nLimiterIter    3;
        alphaApplyPrevCorr  no;

        solver          PBiCG;
        preconditioner  DILU;
        tolerance       1e-15;
        relTol          0;
		nAlphaCorr      1;
    }

    pcorr
    {
        solver          PCG;
        preconditioner  DIC;
        tolerance       1e-5;
        relTol          0;
    }

    p_rgh
    {
        solver          GAMG;
        tolerance       1e-09;
        relTol          0.0001;
        smoother        DIC;
        nPreSweeps      0;
        nPostSweeps     2;
        nFinestSweeps   2;
        cacheAgglomeration true;
        nCellsInCoarsestLevel 10;
        agglomerator    faceAreaPair;
        mergeLevels     1;
    }

    p_rghFinal
    {
        $p_rgh;
        tolerance       1e-9;
        relTol          0;
    }

}

PIMPLE
{
    momentumPredictor no;
    nCorrectors     1;
    nOuterCorrectors 1;
    nNonOrthogonalCorrectors 0;
    pRefCell        0;
    pRefValue       0;
}


