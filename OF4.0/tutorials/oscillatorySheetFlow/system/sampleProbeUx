FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      sampleDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

type probes;
 
	libs ("libsampling.so");

	interpolationScheme cellPointFace; 

	setFormat       gnuplot;

fields
(
	Ux
);

probeLocations	( 
					(0.001 0.45  0)
				);


